template
  require(from="./card-title.css")
  section.mdc-card__primary(class.bind="class" ref="elementPrimary")
    slot
      span(ref="isAvatar")
    h1.mdc-card__title(if.bind="showTitle" class.bind="showLarge ? 'mdc-card__title--large' : ''") ${title}
    h2.mdc-card__subtitle(if.bind="showSubtitle") ${subtitle}
